	"version": "0.1.0",
	"configurations": [
		{
			"name": "Launch Edge",
			"request": "launch",
			"type": "msedge",
			"url": "http://localhost:8080",
			"webRoot": "${workspaceFolder}"
		},
		{
			"name": "Live Server",
			"type": "msedge",
			"request": "launch",
			"url": "http://localhost:5500",
			"webRoot": "${workspaceFolder}"
		},
		{
			"name": "Electron",
			"type": "node",
			"request": "launch",
			"cwd": "${workspaceFolder}",
			"runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron",
			"windows": {
			  "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron.cmd"
			},
			"args" : ["."]
		  }
	]
}